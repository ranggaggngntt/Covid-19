<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid_19">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.AddPrecaution"></activity>
        <activity android:name=".Activity.AddSymptomsActivity" />
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PencegahanActivity" />
        <activity android:name=".Activity.GejalaActivity" />
        <activity android:name=".Fragment.CountryFragment" />
        <activity android:name=".Fragment.AboutFragment" />
        <activity android:name=".Fragment.ProvinsiFragment" />
        <activity android:name=".Fragment.HomeFragment" />
        <activity android:name=".Activity.KnowMoreActivity" />
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>